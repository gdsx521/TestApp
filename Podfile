#使用清华大学源
source 'https://mirrors.tuna.tsinghua.edu.cn/git/CocoaPods/Specs.git'

# Podfile
use_frameworks!

target 'TestApp' do
    pod 'RxSwift'
    pod 'RxCocoa'
end


# RxTest and RxBlocking make the most sense in the context of unit/integration tests
target 'TestAppTests' do
    pod 'RxBlocking'
    pod 'RxTest'
end
